function cityPopUpShow(){
	$('.navbar-collapse.collapse.in').removeClass('in');
	if(parseInt($('#dropdownMenuCart .badge').text())==0){
		$.post('/sell/getModalCity.php',{},function(res){
			$('#citySelectModalBody').html(res);
		});
	}else{
		$('#citySelectModalBody').html("You have product in your cart. So can not change city.");
	}	
	$('#citySelectModal').modal('show');
}
