	 $("#menu_item_how").click(function(){window.location.href = '/./how-it-works.php';});
	 $("#menu_item_ewaste").click(function(){window.location.href = '/./what-is-e-waste.php';});
	 $("#menu_item_aboutus").click(function(){window.location.href = '/./about.php';});
	 

	/* On Scroll Marker Images Chanage Code */
	$('.navbar-standard').addClass('default');
	$('.dropdown.open').css("opacity","none!important");
	$(window).scroll(function(){
		if ($(document).scrollTop() > 100){
			$(".marker-dark").show();
			<!--$(".marker-white").hide();-->
			$('.navbar-standard').removeClass('default');
			$('.navbar-standard').addClass('opaque');
			$('#breadcrumb').addClass('scrollBreadcrumb');
			$('#breadcrumb').removeClass('defaultBreadcrumb');
		}else{
			$('.navbar-standard').addClass('default');
			$('.navbar-standard').removeClass('opaque');
			<!-- $(".marker-white").show(); -->
			<!--$(".marker-dark").hide(); -->
			$('#breadcrumb').addClass('defaultBreadcrumb');
			$('#breadcrumb').removeClass('scrollBreadcrumb');
		}
	});
	isCookie = getCookie('ICUID');
	if (isCookie == null) {
		queryString = "company=107";
		if(checkPerameterExist("utm_source", queryString)){
			date = new Date;
			ctime = date.getTime();
			$.post('zw_admin/getData.php',{'type':'setUTMDB', 'IP':'27.147.171.72', 'ctime':ctime, 'deviceType':'web', 'UTMData': queryString, 'additionalInfo':'', 'version':''}, function (res){
				// console.log(res);
				response=$.parseJSON(res);
				if(response['status']=='Success'){
					createCookie("ICUID", response['msg'], 30);
				}
			});
		}
	}else {
		$('.utmUniqueId').val(isCookie);
		// console.log(isCookie);
	}
});
