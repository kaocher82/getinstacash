	function goforit(a){
		console.log($(a).attr('sp'));
	}
	function createCookie(name, value, days) {
		var date, expires;
		if (days) {
			date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			expires = "; expires="+date.toGMTString();
		} else {
			expires = "";
		}
		document.cookie = name+"="+value+expires+"; path=/";
	}
	function setUserCountryCookie(flag){
			userCountryCode=document.querySelector("meta[name=userCountryCode]").getAttribute("content").toUpperCase();
			createCookie('ICgeoCross',userCountryCode,7);
			$('#routingPopup').modal('hide');
		if(flag!='stay'){
			window.location=flag;	
		}
	}
	function getCookie(name) {
		var dc = document.cookie;
		var prefix = name + "=";
		var begin = dc.indexOf("; " + prefix);
		if (begin == -1) {
			begin = dc.indexOf(prefix);
			if (begin != 0) return null;
		}
		else
		{
			begin += 2;
			var end = document.cookie.indexOf(";", begin);
			if (end == -1) {
			end = dc.length;
			}
		}
		// because unescape has been deprecated, replaced with decodeURI
		//return unescape(dc.substring(begin + prefix.length, end));
		return decodeURI(dc.substring(begin + prefix.length, end));
	} 
	function checkPerameterExist(param, queryString){
		sURLVariables = queryString.split('&');
		for (i = 0; i < sURLVariables.length; i++) {
			sParameterName = sURLVariables[i].split('=');
			if (sParameterName[0] === param) {
				return sParameterName[1] === "" ? false : true;
			}
		}
		return false;
	}
