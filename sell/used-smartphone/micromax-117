<script>
	$('body').on('click', function(res){
		$('#browse').css('display','none');;
		$('#browse1').css('display','none');;
	});
	$(window).scroll(function() {
		$('#browse').css('display','none');;
		$('#browse1').css('display','none');;
	});
	function showNew(){
		$('.browse').css('display','block');
		var name=$('.search').val();
		if(name==''){
			name=$('.search1').val();
		}
		// alert(name);
		var str='';
		$.post('/sell/getAllBrands.php',{'name':name},function (res){
			str=str+res;
			$.post('/sell/getAllProducts.php',{'name':name},function (res1){
				str=str+res1;
				// alert(str);
				$('#browse').html(str);
				$('#browse1').html(str);
			});
		});
	}
	$('.browse').mouseleave(function(){
		$('.browse').css('display','none');
	});
	$('.cart_list >li').each(function() {
		a='';
		var a=$('.indiaAmount', this).text();
		if(!isNaN(a)){
			a = a.trim();
			leng = a.length; //lenght of the no
			if(leng > 0){
				var num = "";
				var b = 0;
				num = a.substring(leng-3,leng); //get the last 3 digits
				b = parseInt(a/1000); //omit the last 3 digits already stored in $num
				while(b > 0){ //loop the process - further get digits 2 by 2
					b=b+"";
					len = b.length;
					num = b.substring(len-2,len)+","+ num;
					b = b.substring(0,len-2);
				}
				$('.indiaAmount', this).text(num);
			}
		}
	});
	$('.total').each(function() {
		a='';
		var a=$('.indiaAmount', this).text();
		if(!isNaN(a)){
			a = a.trim();
			leng = a.length; //lenght of the no
			if(leng > 0) {
				var num = "";
				var b = 0;
				num = a.substring(leng-3,leng); //get the last 3 digits
				b = parseInt(a/1000); //omit the last 3 digits already stored in $num
				while(b > 0){ //loop the process - further get digits 2 by 2
					b=b+"";
					len = b.length;
					num = b.substring(len-2,len)+","+ num;
					b = b.substring(0,len-2);
				}
				$('.indiaAmount', this).text(num);
			}
		}
	});
	function search(a){
