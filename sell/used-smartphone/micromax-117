$(document).ready(function(){
	//Check routing required or not	
	if(document.querySelector("meta[name=userCountryCode]")){
	var metaCountryCode = document.querySelector("meta[name=userCountryCode]").getAttribute("content").toUpperCase();
		if( metaCountryCode === "MY"){
			$('#routingPopup').modal('show');
			$('#routingPopupMY').show();
		}else if(metaCountryCode === "SG"){
			$('#routingPopup').modal('show');
			$('#routingPopupSG').show();
		}
	}
	islogin=$('#menu_item_myaccount').text();
	isProduct=parseInt($('#dropdownMenuCart .badge').text());
	if(islogin!='' && isProduct>0){
		$.post('/sell/checkCartSession.php',{},function(res){
			if(res=='Success'){
				$('#oldCartConfirmAll').modal('show');
			}
		});
	}
	if('no'=='no'){
		ga('send', 'pageview'); 
	}
	$(".search_button").on("click", function(){
			ga('send', 'event', { eventCategory: 'GetInstaCash Calc', eventAction: 'Navigation search'});
	});
	
	$('body').on('click', function(){
		$('#browse').css('display','none');
	});
	$('#searchForm').submit(function(evnet){
		itemv=$('#search').val();
		window.location='/sell/productSearch/'+itemv
		event.preventDefault();
	});
	
	if('product.php'!='login.php'){	
			$.post('/sell/getModalCity.php',{},function(res){
			$('#citySelectModalBody').html(res);
			$('#citySelectModal').modal('show');
		});
		}
	$(".footercol").click(function() {
		$('html, body').animate({
			scrollTop: $(".copyright").offset().top
		},500);
		$(".fa-caret-square-o-down").css("margin-top","-23px");
	});
	$("#footerBox").on("hide.bs.collapse", function(){
		$('html, body').animate({
			scrollTop: $(".copyright").offset().top
		},500);
		$(".changecss").css("margin-top","-10px");
		$(".footercol").html('<i style="font-size: 2em; text-align: center;color:#222222;position:absolute; margin-top:-23px;" class="fa fa-caret-square-o-up"></i>');
	});
	$("#footerBox").on("show.bs.collapse", function(){
		$(".footercol").html('<i  style="font-size: 2em; text-align: center;color:#222222;position:absolute; margin-top:-23px;" class="fa fa-caret-square-o-down"></i>');
		$(".changecss").css("margin-top","-10px");
	});
	$('#mobileModalForm').submit(function(event){ 
		ga('send', 'event', { eventCategory: 'Product Detail', eventAction: 'Mobile Number Selection', eventLabel: 'Mobile Number', eventValue: $('#mobileModalForm #popupMobileNumber').val()});
		$.post('/sell/setSessoinCity.php',$('#mobileModalForm').serialize(),function(res){ 
			$('#mobileModal').modal('hide');
			$('#conditionCheck').trigger('submit');
		});
		event.preventDefault();	
	});
});
function arrangeCartAll(flag){
	$('#oldCartConfirmAll').modal('hide');
	$.post('/sell/correctCart.php',{"flag":flag},function(res){
		if(res=='Success'){
			// alert(res);
			window.location.reload();
		}
	});
}
function setMainPin(pin,a){
	$('#mainPin').val(pin);
	$('.modalPopCityBlock').removeClass('cityButtonSelected');
	$(a).addClass('cityButtonSelected');
	$('#goPin').removeClass('btn-default').addClass('btn-primary');
	$('#goPin').prop('disabled',false);
	checkPincodeAvailibility(pin);
}
function changeModalCity(a,b){
	$('#hiddenModalCity').val(a);
	$('#hiddenModalCity1').val(b);
	ga('send', 'event', { eventCategory: 'City Popup', eventAction: 'City Selection', eventLabel: 'City Name', eventValue: b});
	$.post('/sell/setSessoinCity.php',{'city' :a}, function(res){
		$('#citySelectModal').modal('hide');
		if(b!=''){
			$('#inviteshw').html('<a href="#" style="cursor:pointer" onclick="cityPopUpShow();"> <div style="line-height:41px;"><img src="/sell/img/marker-g.png"  class="marker-white" alt="" />&nbsp; <span class="badge" style="background-color:#aaa;color:#fff;margin-bottom: 9px;margin-top:7.5px;">'+b+'  </span></div></a>');
		}
			$.post('/sell/checkSession.php',{'type':'button'},function(res){
				if(res=='cal'){
					$('#conditionCheck').trigger('submit');
				}
			});
	});
	//$('#cityModalRow1').hide();
	//$('#cityModalRow2').show();
}
