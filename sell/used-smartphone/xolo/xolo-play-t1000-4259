						id=$('input[name="'+name+'"]:checked').nextAll('input').attr('id');
						str1=str1+"<td>"+$('#'+id).next('.summaryData').html()+"</td></tr>";
						strt1=strt1+$('#'+id).next('.summaryData').text()+';';
						cestr1=$('#'+id).next('.summaryData').html();
					}else{
						str1=str1+"<td>"+$('#'+id).next('.summaryData').html()+"</td></tr>";
						strt1=strt1+$('#'+id).next('.summaryData').text()+';';
					}
					if($('input[name="'+name+'"]:checked').nextAll('input').nextAll('input').attr('name')==name && $('input[name="'+name+'"]:checked').nextAll('input').nextAll('input').attr('special').substring(0,1=='a')){
						id=$('input[name="'+name+'"]:checked').nextAll('input').nextAll('input').attr('id');
						str2=str2+"<td>"+$('#'+id).next('.summaryData').html()+"</td></tr>";
						strt2=strt2+$('#'+id).next('.summaryData').text()+';';
						cestr2	=$('#'+id).next('.summaryData').html();
