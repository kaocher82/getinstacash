						var tmp = document.createElement("DIV");
						tmp.innerHTML = $(this).attr('sp');
						strt=strt + tmp.textContent || tmp.innerText;
						strt+=";";
						cestr=$(this).attr('sp');
						hd=$(this).parent().find('.panel-body').html();
