	  }
	   function addToCartforCheck(){
		   track=1;
		   isProduct=parseInt($('#dropdownMenuCart .badge').text());
		   if(isProduct==0){
		   var te=$('#moveToCheckOut').text();
		   if(te=='Recycle Now'){
			    ga('send', 'event', { eventCategory: 'Product Detail', eventAction: 'Recycle Now Clicked', eventLabel: $('#pName').text(), eventValue: $('#tAmount').val()});
		   }else{
				ga('send', 'event', { eventCategory: 'Product Detail', eventAction: 'Add to Cart & Checkout Clicked', eventLabel: $('#pName').text(), eventValue: $('#tAmount').val()});
				fbq('track', 'AddToCart', {content_ids: [4259],content_type: 'product',value: $('#tAmount').val(),currency: 'INR'});
				fbq('track', 'InitiateCheckout');
		   }
			ga('ec:addProduct', {
			'id': 4259,
			'name': 'Xolo Play T1000',
			'category': 'smartphone',
			'brand': 'XOLO',
			'price': $('#pa').val(),
			'quantity': 1
		  });
		  ga('ec:setAction', 'add');
		  ga('send', 'pageview');
			/*var popover =$("[data-toggle=popover1]").attr('data-content',hd1).data('bs.popover');
			popover.setContent();
			popover.$tip.addClass(popover.options.placement);
			var popover =$("[data-toggle=popover2]").attr('data-content',hd2).data('bs.popover');
			popover.setContent();
			popover.$tip.addClass(popover.options.placement);*/
			var popover =$("[data-toggle=popover3]").attr('data-content',hd).data('bs.popover');
			popover.setContent();
			popover.$tip.addClass(popover.options.placement);
			$('#popBlock1').show();
			$('#popBlock2').hide();
			$("#alternateOption").modal('show');
		    //$("#movefor").on('click',goCheckout);
		   }else{
			   $('#oldCartConfirm').modal('show');
		   }
		  
	  }
	  $('#movefor').click(function(){
		  $('#movefor').prop('disabled', true);
		  if(track==0){
			  goCategory();
		  }if(track==1){
			  goCheckout();
		  }
	  });
	  function goCategory(){
		   $.post('/sell/setSessoinCity.php',{'type':'button', 'val':'category'},function(res){
		  });
		  $.post('/sell/addToCart.php', $('#conditionCheck').serialize(),function(res){
			  //alert(res);
			 myOrder = res.split('@@@');
			res = myOrder[0];
			//alert(res);
			plid='';
			if(myOrder.length>1){
				txnDetail = (myOrder[1].split('***')[0]).split('@#');
				plid=txnDetail[0];
				myItem=(myOrder[1].split('***')[1]).split('@#');
				ga('ec:addProduct', {               // Provide product details in an productFieldObject.
				  'id': myItem[0],                   // Product ID (string).
				  'name': 'Xolo Play T1000', // Product name (string).
				  'category': 'smartphone',            // Product category (string).
				  'brand': 'XOLO',                // Product brand (string).
				  'price': 'myItem[4]',                 // Product price (currency).
				  'quantity': 1                     // Product quantity (number).
				});
				ga('send', 'pageview');
			}
			//alert(myOrder.length);
			//alert(myOrder[1]);
			//alert(myOrder[1].split('***')[0]);
			//alert(myOrder[1].split('***')[1]);
			 if(res=='fraud'){ 
				window.location='/sell/error-500.php';
			 }else if(res=='Success'){
				window.location='/sell/login/';
			 }else if(res=='SuccessLogin'){
				window.location='/sell/priceLockSuccess/'+plid+'/';
			 }
		  });
	  }
	  function goCheckout(){
		  $.post('/sell/setSessoinCity.php',{'type':'button', 'val':'checkOut'},function(res){
		  });
		  $.post('/sell/addToCart.php', $('#conditionCheck').serialize(),function(res){
			  console.log(res);
			 if(res=='fraud'){ 
				window.location='/sell/error-500.php';
			 }else if(res=='Success'){
				window.location='/sell/login/';
			 }else if(res=='SuccessLogin'){
				window.location='/sell/checkOut/';
			 }
		 });
	  }
	  function moneyConvert(bp){
		  var a=bp.toString();
			a = a.trim();
			leng = a.length; //lenght of the no
			if(leng > 0) {
				var num = "";
				var b = 0;
				num = a.substring(leng-3,leng); //get the last 3 digits
				b = parseInt(a/1000); //omit the last 3 digits already stored in $num
				while(b > 0) //loop the process - further get digits 2 by 2
				{
					b=b+"";
					len = b.length;
					num = b.substring(len-2,len)+","+ num;
					b = b.substring(0,len-2);
				}
			}
				return num;
	  }
	
	if($(window).width() <= 768){
		//alert('hi');
		$('.panel-heading span.clickable').parents('.panel').find('.panel-body').slideUp();
		$('.panel-heading span.clickable').addClass('panel-collapsed');
		if($('.panel-heading span.clickable').find('i').hasClass('fa-minus')){
			$('.panel-heading span.clickable').find('i').removeClass('fa-minus').addClass('fa-plus');
		}
	}
    jQuery(function ($) {
        $('#summaryCollapse').on("click", function (e) {
            if ($(this).hasClass('panel-collapsed')) {
                // expand the panel
                $(this).parents('.panel').find('.panel-body').slideDown();
                $(this).removeClass('panel-collapsed');
                $(this).find('i').removeClass('fa-plus').addClass('fa-minus');
            }
            else {
                // collapse the panel
                $(this).parents('.panel').find('.panel-body').slideUp();
                $(this).addClass('panel-collapsed');
                $(this).find('i').removeClass('fa-minus').addClass('fa-plus');
            }
        });
    });
</script>
