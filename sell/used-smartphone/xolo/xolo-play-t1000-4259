							str1=str1 + $(this).parent().parent().next().find('input').attr('sp')+"<br>";
							var tmp = document.createElement("DIV");
							tmp.innerHTML = $(this).parent().parent().next().find('input').attr('sp');
							strt1=strt1 + tmp.textContent || tmp.innerText || "" + ";";
							cestr1=$(this).parent().parent().next().find('input').attr('sp');
							hd1=$(this).parent().parent().next().find('.panel-body').html();
						}else{
							str1=str1 + $(this).attr('sp')+"<br>";
							strt1=strt1 + $(this).attr('sp')+";";
						}
						if($(this).parent().parent().next().next().find('input').attr('name')==aname && ($(this).parent().parent().next().next().find('input').attr('special')).substring(0,1)=='a'){
