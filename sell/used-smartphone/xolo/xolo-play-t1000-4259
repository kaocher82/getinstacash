		});
		
		str=str+"</table>";
		str1=str1+"</table>";
		str2=str2+"</table>";
		$('#summary').val(str);
		$('#description').val(strt);
		$('#summaryView').html(str);
	 }
	function arrangeCart(flag){
		if(flag==0){  
			$.post('/sell/clearCart.php',{},function(res){ 
				$('#oldCartConfirm').modal('hide');
				alert(res);
				$('#dropdownMenuCart .badge').text('0');
				addToCartforCheck();
			});
		}else{
			window.location='/sell/checkOut/'
		}		
	}
		
