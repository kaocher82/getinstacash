	$(".h_search_close a").click(function(){
		$value = $(".h_search_form").attr("style");
		if($value == 'height: 54px;'){
			$(".defaultBreadcrumb").css("margin-top","72px");
			$(".scrollBreadcrumb").css("margin-top","58px");
		}
		if($value == 'height: 0px;'){
			$(".defaultBreadcrumb").css("margin-top","126px");
			$(".scrollBreadcrumb").css("margin-top","72px");
		}
		if($value === undefined){
			$(".defaultBreadcrumb").css("margin-top","126px");
			$(".scrollBreadcrumb").css("margin-top","72px");
		}
	});
	
