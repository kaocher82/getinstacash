			$('#cestr2').html('');
			$('#to2').html('');
			$('#alternateBody').hide();
			if(nwid!='0'){
			$('#alternateBody2').show();
			$('#alternateBody2').html('<div align="center">Not working / Dead / Scrap <a style="color:#2a6496;" href=/sell/productDetail.php?id='+nwid+'>click here<\a> to know value.</div>');
			}
		}
		$('#balance').show();
		ga('send', 'event', { eventCategory: 'Product Detail', eventAction: 'Calculate Button Clicked', eventLabel: $('#pName').text(), eventValue: num});
		 gtag('event', 'page_view', {'send_to': 'AW-1011350210',
		   'ecomm_prodid': '4259',
		   'ecomm_pagetype': 'Product Detail',
		   'ecomm_totalvalue': bp
		  });
		fbq('track', 'ViewContent', {content_ids: 4259,content_type: 'product',value: num,currency: 'INR'});
		ga('require', 'ec');
		ga('ec:setAction', 'click', {            // Provide product details in an impressionFieldObject.
		  'id': 4259,                   // Product ID (string).
		  'name': $('#pName').text(), // Product name (string).
		  'category': 'smartphone',   // Product category (string).
		  'brand': 'XOLO'               // Product brand (string).
		});
		ga('send', 'pageview'); 
		if($('#utCounter').val()<2){
		 $.post('/sell/addUserTracking.php',$('#conditionCheck').serialize(),function(res){
			 
		  });
		  $('#utCounter').val($('#utCounter').val()+1);
		  
		}
		}); 
		 }
		  }); 
		}
		});
		event.preventDefault();
			
	  });
	   switchIcons();
	   showSummary();
	});	
	
	function switchIcons(){
		$("#conditionCheck").find(":input").each(function(){
			//swtich 
			if($(this).attr('name')=="switch-radio1"){
				swId=$(this).attr("id");
				$switchId='';
				sonstr=$('input[name="'+swId+'"][cap=on]').attr('sp');
				soffstr=$('input[name="'+swId+'"][cap=off]').attr('sp');
				$('#'+swId).switchButton({
					width: 100,
					height: 25,
					button_width: 70,
					on_label: sonstr,
					off_label: soffstr
				});
			}
		});
	}
	 function showSummary(){
