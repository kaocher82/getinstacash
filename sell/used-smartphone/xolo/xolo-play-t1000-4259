	});
});
function arrangeCartAll(flag){
	$('#oldCartConfirmAll').modal('hide');
	$.post('/sell/correctCart.php',{"flag":flag},function(res){
		if(res=='Success'){
			// alert(res);
			window.location.reload();
		}
	});
}
function setMainPin(pin,a){
	$('#mainPin').val(pin);
	$('.modalPopCityBlock').removeClass('cityButtonSelected');
	$(a).addClass('cityButtonSelected');
	$('#goPin').removeClass('btn-default').addClass('btn-primary');
	$('#goPin').prop('disabled',false);
	checkPincodeAvailibility(pin);
}
function changeModalCity(a,b){
	$('#hiddenModalCity').val(a);
	$('#hiddenModalCity1').val(b);
	ga('send', 'event', { eventCategory: 'City Popup', eventAction: 'City Selection', eventLabel: 'City Name', eventValue: b});
	$.post('/sell/setSessoinCity.php',{'city' :a}, function(res){
		$('#citySelectModal').modal('hide');
		if(b!=''){
			$('#inviteshw').html('<a href="#" style="cursor:pointer" onclick="cityPopUpShow();"> <div style="line-height:41px;"><img src="/sell/img/marker-g.png"  class="marker-white" alt="" />&nbsp; <span class="badge" style="background-color:#aaa;color:#fff;margin-bottom: 9px;margin-top:7.5px;">'+b+'  </span></div></a>');
		}
			$.post('/sell/checkSession.php',{'type':'button'},function(res){
				if(res=='cal'){
					$('#conditionCheck').trigger('submit');
				}
			});
	});
	//$('#cityModalRow1').hide();
	//$('#cityModalRow2').show();
}
